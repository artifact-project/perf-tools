{
  "name": "@perf-tools/keeper",
  "version": "2.0.0-rc.23",
  "description": "PerfKeeper â€” It's a tool for performance monitoring and profiling your application (also best replacement for `console.time`).",
  "keywords": [
    "perf-tools",
    "performance",
    "profiling",
    "navigation",
    "paint",
    "timings",
    "fps",
    "keeper"
  ],
  "author": "RubaXa <ibnRubaXa@gmail.com",
  "license": "MIT",
  "repository": "git@github.com:artifact-project/perf-tools.git",
  "homepage": "https://github.com/artifact-project/perf-tools/tree/master/keeper",
  "iife": "dist/keeper.iife.min.js",
  "module": "dist/keeper.es.js",
  "main": "dist/keeper.cjs.js",
  "scripts": {
    "start": "npm run build; echo 'http://localhost:2312/'; python -m SimpleHTTPServer 2312;",
    "test": "jest",
    "jest-test:size": "npm run build:rollup && npm run size",
    "ts-build": "tsc",
    "size": "size-limit && npm run size:raw",
    "size:raw": "ls -al dist/*.js | awk '{ print \" - \" $9 \" -> \" $5 \" B\"; }' && echo \"\"",
    "build:rollup": "NODE_ENV=production rollup --config",
    "build": "npm run build:rollup && npm run size",
    "post-build": "node ./post-build.js && npm run clean-dist",
    "clean-dist": "rm -f ./dist/*.d.ts && rm -rf ./dist/*/",
    "git-clean": "git clean -f",
    "prepublishOnly": "npm run test && npm run build && npm run ts-build",
    "postpublish": "npm run git-clean",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@size-limit/preset-small-lib": "^4.6.0",
    "@types/jest": "^25.2.3",
    "jest": "^25.5.4",
    "rollup": "^2.26.10",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "^6.0.0",
    "size-limit": "^4.6.0",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.7",
    "uglify-js": "^3.10.4"
  },
  "size-limit": [
    {
      "limit": "2.61 KB",
      "path": "dist/keeper.iife.min.js"
    }
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "\\.tests\\.ts$",
    "transformIgnorePatterns": [],
    "setupFilesAfterEnv": [],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
