{
	"compilerOptions": {
	  "target": "ES5",
	  "module": "commonjs",
	  "jsx": "react",
	  "lib": [
      	"dom"
	  ],  
	  "declaration": true,
	  "isolatedModules": true,
	  "importsNotUsedAsValues": "error",
	  "strict": true,
	  "noUnusedParameters": true,
	  "noImplicitReturns": false,
	  "noFallthroughCasesInSwitch": true,
	  "moduleResolution": "node",
	  "esModuleInterop": false,
	  "resolveJsonModule": true,
	  "sourceMap": true,
	  "skipLibCheck": true
	},
	"exclude": [
	  "node_modules"
	]
}
